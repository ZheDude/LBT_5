en
conf t
hostname ISP2-PE-2
key chain OSPF
key 1
cryptographic-algorithm hmac-sha-512
key-string cisco

end
conf t
ip domain name 5CN

username cisco priv 15 algo scrypt secret cisco
crypto key gen rsa mod 4096 usage
ip ssh version 2

line vty 0 15
login local
transport input ssh
logging synch
exec-timeout 0 0
end
conf t

int g0/0
ip ospf authentication key-chain OSPF
ip add 192.168.2.1 255.255.255.0
desc to ISP2-BB-2
no shut

int lo0
ip add 1.1.2.2 255.255.255.255
no shut

int g0/1
ip add 52.235.12.1 255.255.255.252
no shut

router ospf 1
router-id 1.1.2.2
network 192.168.2.0 0.0.0.255 area 0
network 1.1.2.2 0.0.0.0 area 0
mpls ldp autoconfig




router bgp 20
bgp router-id 1.1.2.2

#IBGP Zeug
neighbor 1.1.2.3 remote-as 20
neighbor 1.1.2.3 update-source lo0

neighbor 1.1.2.1 remote-as 20
neighbor 1.1.2.1 update-source lo0
network 52.235.12.0 mask 255.255.255.252

# NEU
# VRF configuration:
ip vrf Cust-A
rd 10:50
route-target export 10:50
route-target import 10:50

int gig0/1
ip vrf forwarding Cust-A
ip add 52.235.12.1 255.255.255.252

router bgp 20
no neighbor 1.1.2.3 remote-as 20
no neighbor 1.1.2.3 update-source lo0

no neighbor 1.1.2.1 remote-as 20
no neighbor 1.1.2.1 update-source lo0
no network 52.235.12.0 mask 255.255.255.252

neighbor PE1_PE3 peer-group
neighbor 1.1.2.1 peer-group PE1_PE3
neighbor 1.1.2.3 peer-group PE1_PE3

neighbor PE1_PE3 remote-as 20
neighbor PE1_PE3 update-source lo0
address-family vpnv4
neighbor PE1_PE3 activate
neighbor PE1_PE3 send-community extended